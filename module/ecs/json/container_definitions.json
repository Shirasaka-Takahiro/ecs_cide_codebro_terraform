[
    {
        "name": "${project}-${env}-${cluster_role}",
        "image": "${ecr_repository_url}",
        "essential": true,
        "portMappings": [
            {
                "containerPort": 80,
                "hostPort": 80
            }
        ],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
              "awslogs-region": "ap-northeast-1",
              "awslogs-stream-prefix": "${project}-${env}-${cluster_role}",
              "awslogs-group": "${cw_log_group}"
            }
          }
    }
]